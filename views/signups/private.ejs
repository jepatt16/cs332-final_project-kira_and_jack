<main class="p-3">
  <h3 class="pb-3">Pending open mic performers</h3>
  <table class="table">
    <thead>
      <tr>
        <th class="border-0">Name</th>
        <th class="border-0">Description</th>
        <th class="border-0">Date</th>
        <th class="border-0">Time</th>
        <th class="border-0">id</th>
        <th class="border-0">Actions</th>
      </tr>
    </thead>
    <tbody>
      <%_ for (const signup of signups) { -%>
        <tr>
            <td><%= signup.name -%></td>
            <td><%= signup.description -%></td>
            <td><%= signup.date -%></td>
            <td><%= signup.time -%></td>
            <td><%= signup.id -%></td>
            <td><button class="approve btn btn-warning">Approve</button></td>
            <td><button class="delete btn btn-warning">Delete</button></td>
        </tr>
      <%_ } -%>
    </tbody>
  </table>
</main>
