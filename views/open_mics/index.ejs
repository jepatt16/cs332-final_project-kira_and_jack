<!DOCTYPE html>
<html>
  <%- include('../head'); -%>
  <body>
    <%- include('../navbar', {active: 'open_mics'}); -%>
    <main class="p-3">
      <table class="table">
        <thead>
          <tr>
            <th class="border-0">Date</th>
          </tr>
        </thead>
        <tbody>
          <%_ for (const event of events) { -%>
            <tr>
                <td><a href="/open_mic/<%=event.date-%>"><%= event.date -%></a></td>
            </tr>
          <%_ } -%>
        </tbody>
      </table>
    </main>
  </body>
</html>
